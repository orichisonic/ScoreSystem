<script language="javascript" type="text/javascript">
function jump(p_id) {
   if ($.trim($("#login_uaccount").text()) == "") {
      alert("您还未登陆9you帐号，请先登陆！");
      return false;
   }
   //location.href("http://"+location.host+"/CardPush.aspx/stepfour");
   if (p_id == 1) {
      //jw2
      location.href("http://" + location.host + "/CardPush.aspx/jw2item");
   }
   else if (p_id == 2) {
      //sdo
      location.href("http://" + location.host + "/CardPush.aspx/sdoitem");
   }
   else if (p_id == 3) {
      //sdo
      location.href("http://" + location.host + "/CardPush.aspx/auitem");
   }
   else if (p_id == 4) {
      //au
      location.href("http://" + location.host + "/CardPush.aspx/gditem");
   }
   else if (p_id == 5) {
      //lq
      location.href("http://" + location.host + "/CardPush.aspx/BasketballItem");
   }
   else if (p_id == 6) {
      //DNH
      location.href("http://" + location.host + "/CardPush.aspx/DNHSSNEW");
   }
}

</script>
